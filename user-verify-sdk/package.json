    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "eslint": "^9.39.1",
    "eslint-plugin-preact": "^0.1.0",
    "prettier": "^3.7.3",
    "typescript": "~5.9.3",
    "vite": "^7.2.4"
  },
  "files": ["dist"],
  "main": "./dist/user-verify-sdk.umd.js",
